<Application x:Class="wpftdm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Shared="False"  x:Key="Locator" xmlns:vm="clr-namespace:wpftdm.Core" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Content\Styles\BureauBlue.xaml" />
                <ResourceDictionary Source="Content\Styles\Custom.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
